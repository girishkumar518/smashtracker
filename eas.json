{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      }
    },
    "production": {
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      },
      "android": {
         "buildType": "app-bundle"
      }
    },
    "apk": {
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
